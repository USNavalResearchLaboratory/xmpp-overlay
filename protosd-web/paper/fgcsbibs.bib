@article{keahey,
	Annote = {The increasing feasibility of developing
          applications{\newline} spanning nationwide supercomputing
          resources makes possible{\newline} the creation of
          simulations composed of multiple{\newline}
          interdisciplinary components and capable of
          modeling{\newline} natural and social phenomena of national
          importance with{\newline} unprecedented speed and accuracy.
          However, the potential{\newline} offered by hardware
          technology often fails to be fully{\newline} realized due
          to the lack of software environments{\newline} supporting
          such efforts. Furthermore, the complexity of{\newline}
          combining within one application components with
          different{\newline} performance characteristics often
          prevents such{\newline} applications from achieving
          required performance levels.{\newline} The Ligature project
          at LANL addresses the issue of{\newline} designing a
          software infrastructure enabling fats and{\newline}
          efficient development of multi-component applications,
          and{\newline} that of providing performance guidance to the
          programmer{\newline} using this infrastructure. Ligature
          allows the programmer{\newline} to define component
          interfaces specifying how{\newline} heterogeneous,
          distributed components can interact within a{\newline}
          larger system and provides a reusable
          infrastructure{\newline} capable of connecting these
          components. These interfaces,{\newline} as well as
          information about component performance are{\newline}
          accessible through a database. Within this framework we
          are{\newline} trying to understand how information about
          the performance{\newline} of individual components, and
          information about performance{\newline} of the framework
          can be combined to develop a{\newline} performance-aware
          multicomponent application.},
	Author = {Keahey, Katazyna and Beckman, Peter and Ahrens, James},
	Title = {Ligature: Component Architecture for High-Performance Applications}}

